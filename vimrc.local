"path etc/vim/vimrc.local
call pathogen#infect()
"filetype plugin indent on
set tabstop=4
set softtabstop=4
set shiftwidth=4
set expandtab
set number
let Tlist_Ctags_Cmd='/usr/bin/ctags'
nnoremap <silent> <F8> :TlistToggle <CR>
map <F3> :source /home/zj/vimscript/erlangFunctionAnnotation.vim<CR>
map erlfn :source /home/zj/vimscript/erlangFunctionAnnotation.vim<CR>
map erln :source /home/zj/vimscript/erlangAnnotationHead.vim<CR>


